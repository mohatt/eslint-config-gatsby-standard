  "license": "MIT",
  "homepage": "https://github.com/mohatt/eslint-config-gatsby-standard",
  "bugs": "https://github.com/mohatt/eslint-config-gatsby-standard/issues",
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-jsx": "^10.0.0",
